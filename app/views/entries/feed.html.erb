<p id="notice"><%= notice %></p>


<div class="h1">Today's Journal Entries</div>


<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Entryname</th>
      <th>Country</th>
      <th>City</th>
      <th>Content</th>
      <th>Points</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @entries.each do |entry| %>
      <tr>
        <td><%= entry.username %></td>
        <td><%= entry.name %></td>
        <td><%= entry.country %></td>
        <td><%= entry.city %></td>
        <td><%= entry.content %></td>
        <td><%= entry.points %></td>
        <td><%= link_to 'Show', entry %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Entry', new_entry_path %>